<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="false">
    <test name="Regression with logging">
        <groups>
            <run>
                <include name="logging"/>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="org.example.tests.PositiveTests"/>
            <class name="org.example.tests.NegativeTests"/>
        </classes>
    </test>
</suite>